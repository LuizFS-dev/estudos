algoritmo "Exercicio15"
var
   dias, horas, salario: real
inicio
      Escreva("Quantos dias voce trabalhou?")
      leia(dias)
      
      horas <- dias * 8
      salario <- dias * 8 * 25
      
      Escreva("Voce trabalhou por",horas," horas e receberá esse mes, o valor de R$",salario)
fimalgoritmo